{
  "api_keys": {
    "openai_key": "",
    "claude_key": "",
    "llama_key": "",
    "gigachat3_key": "",
    "katya_key": ""
  },
  "models": {
    "default_text_model": "gigachat3-702b",
    "default_embedding_model": "text-embedding-ada-002",
    "openai_models": [
      "gpt-4",
      "gpt-3.5-turbo"
    ],
    "claude_models": [
      "claude-3-opus",
      "claude-3-sonnet"
    ],
    "llama_models": [
      "llama-2-70b",
      "llama-2-13b"
    ],
    "gigachat3_models": [
      "gigachat3-702b"
    ],
    "katya_models": [
      "katya-v1"
    ]
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "logs/aiplatform.log"
  },
  "language": {
    "default": "en",
    "supported": [
      "en",
      "ru",
      "zh",
      "ar"
    ]
  },
  "quantum": {
    "default_qubits": 4,
    "max_qubits": 32,
    "simulator": "qiskit"
  },
  "vision": {
    "default_confidence": 0.5,
    "max_image_size": 10485760,
    "supported_formats": [
      "jpg",
      "jpeg",
      "png",
      "bmp",
      "webp"
    ]
  },
  "security": {
    "did_method": "did:ethr",
    "key_algorithm": "ES256K"
  },
  "protocols": {
    "qmp_port": 8080,
    "timeout": 30
  }
}